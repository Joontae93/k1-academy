{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
	"styles":{
		"typography": {
			"fontFamily":"\"Poppins\", \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\",Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\",\"Noto Color Emoji\""
		},
		"spacing": {
			"blockGap": "2rem"
		
		}
	},
    "settings": {
        "color": {
			"custom":true,
            "palette": [
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                },
				{
					"name":"Academy Green",
					"slug":"primary",
					"color":"#2b3636"
				},
				{
					"name":"Academy Gray",
					"slug":"secondary",
					"color":"#4b4b4b"
				},
				{
					"name":"Dark",
					"slug":"dark",
					"color":"#2a2a2a"
				}
            ],
			"gradients":[
				{
					"name":"Dark Gray Gradient",
					"slug":"dark-gray",
					"gradient":"linear-gradient(to bottom, #4b4b4b 0%, #2a2a2a 100%)"
				},
				{
					"name":"Darken Gradient",
					"slug":"darken",
					"gradient":"linear-gradient(to bottom, transparent 0%, rgba(0,0,0,.25) 100%)"
				},
				{
					"name":"Lighten Gradient",
					"slug":"lighten",
					"gradient":"linear-gradient(to bottom, transparent 0%, rgba(255,255,255,.75) 100%)"
				}
			],
			"defaultGradients": false,
			"defaultPalette": false
        },
		"typography": {
			"lineHeight": true,
			"fontWeight": true,
			"textTransform": true,
			"fontFamilies":[
				{
					"name":"Poppins",
					"slug":"poppins",
					"fontFamily":"Poppins",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "300 400 700",
							"src": "url(https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;700&display=swap)",
							"fontDisplay": "swap"
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "300 400 700",
							"src": "url(https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;700&display=swap)",
							"fontDisplay": "swap"
						}
					]
				}
			]
		},
		"spacing": {
			"blockGap":true,
			"customSpacingSize": true,
			"margin": true,
			"padding": true,
			"units":[
				"px",
				"rem",
				"vw",
				"%"
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1400px"
		}
    }
}
